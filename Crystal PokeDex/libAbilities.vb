    Public Sub InitAbilitySettings()
        InitializeListAbilityInformalCHT()
        InitializeListAbilityOfficialCHT()
        InitializeListAbilityInformalCHS()
        InitializeListAbilityOfficialCHS()
        InitializeAbilityNamesInformalCHT()
        InitializeAbilityNamesOfficialCHT()
        InitializeAbilityNamesInformalCHS()
        InitializeAbilityNamesOfficialCHS()
        InitializeAbilityDescriptions()
        InitializeAbilityNamesENG()
        InitializeAbilityNamesJPN()
        Dim tmp As String
        IsoStore = IO.IsolatedStorage.IsolatedStorageFile.GetUserStoreForApplication
        If Not IsoStore.FileExists("A_CHS") Then
            IsoStoreStream = IsoStore.CreateFile("A_CHS")
            FileWriter = New IO.StreamWriter(IsoStoreStream)
            FileWriter.Write("0")
            FileWriter.Close()
            IsoStoreStream.Close()
        End If
        If Not IsoStore.FileExists("A_OFFICIAL") Then
            IsoStoreStream = IsoStore.CreateFile("A_OFFICIAL")
            FileWriter = New IO.StreamWriter(IsoStoreStream)
            FileWriter.Write("1")
            FileWriter.Close()
            IsoStoreStream.Close()
        End If
        IsoStoreStream = IsoStore.OpenFile("A_CHS", IO.FileMode.OpenOrCreate, IO.FileAccess.ReadWrite)
        FileReader = New IO.StreamReader(IsoStoreStream)
        tmp = FileReader.ReadLine
        If tmp = "1" Then
            IsAbilitySimplifiedChineseEnabled = True
        Else
            IsAbilitySimplifiedChineseEnabled = False
        End If
        FileReader.Close()
        IsoStoreStream.Close()
        IsoStoreStream = IsoStore.OpenFile("A_OFFICIAL", IO.FileMode.OpenOrCreate, IO.FileAccess.ReadWrite)
        FileReader = New IO.StreamReader(IsoStoreStream)
        tmp = FileReader.ReadLine
        If tmp = "1" Then
            IsAbilityOfficialTranslationEnabled = True
        Else
            IsAbilitySimplifiedChineseEnabled = False
        End If
        FileReader.Close()
        IsoStoreStream.Close()
        If IsAbilityOfficialTranslationEnabled Then
            If IsAbilitySimplifiedChineseEnabled Then
                UIAbilityChineseDisplayConfig = ChineseDisplayConfig.OfficialChineseSimplified
            Else
                UIAbilityChineseDisplayConfig = ChineseDisplayConfig.OfficialChineseTraditional
            End If
        Else
            If IsAbilitySimplifiedChineseEnabled Then
                UIAbilityChineseDisplayConfig = ChineseDisplayConfig.InformalChineseSimplified
            Else
                UIAbilityChineseDisplayConfig = ChineseDisplayConfig.InformalChineseTraditional
            End If
        End If
    End Sub
    Public Sub GetAbilityDisplayConfig(IsCHS As Boolean, IsOfficial As Boolean)
        If IsOfficial Then
            If IsCHS Then
                UIAbilityChineseDisplayConfig = ChineseDisplayConfig.OfficialChineseSimplified
            Else
                UIAbilityChineseDisplayConfig = ChineseDisplayConfig.OfficialChineseTraditional
            End If
        Else
            If IsCHS Then
                UIAbilityChineseDisplayConfig = ChineseDisplayConfig.InformalChineseSimplified
            Else
                UIAbilityChineseDisplayConfig = ChineseDisplayConfig.InformalChineseTraditional
            End If
        End If
        If IsoStore.FileExists("A_CHS") Then
            IsoStore.DeleteFile("A_CHS")
        End If
        If IsoStore.FileExists("A_OFFICIAL") Then
            IsoStore.DeleteFile("A_OFFICIAL")
        End If
        If Not IsoStore.FileExists("A_CHS") Then
            IsoStoreStream = IsoStore.CreateFile("A_CHS")
            FileWriter = New IO.StreamWriter(IsoStoreStream)
            If IsCHS Then
                FileWriter.Write("1")
            Else
                FileWriter.Write("0")
            End If
            FileWriter.Close()
            IsoStoreStream.Close()
        End If
        If Not IsoStore.FileExists("A_OFFICIAL") Then
            IsoStoreStream = IsoStore.CreateFile("A_OFFICIAL")
            FileWriter = New IO.StreamWriter(IsoStoreStream)
            If IsOfficial Then
                FileWriter.Write("1")
            Else
                FileWriter.Write("0")
            End If
            FileWriter.Close()
            IsoStoreStream.Close()
        End If
    End Sub
    Public Sub SetCurrentAbilityDisplay()
        Select Case UIAbilityChineseDisplayConfig
            Case ChineseDisplayConfig.DefaultConfig
                CurrentAbilityList = ListAbilityOfficialCHT
                CurrentAbility = AbilityNamesOfficialCHT
            Case ChineseDisplayConfig.InformalChineseSimplified
                CurrentAbilityList = ListAbilityInformalCHS
                CurrentAbility = AbilityNamesInformalCHS
            Case ChineseDisplayConfig.InformalChineseTraditional
                CurrentAbilityList = ListAbilityInformalCHT
                CurrentAbility = AbilityNamesInformalCHT
            Case ChineseDisplayConfig.OfficialChineseSimplified
                CurrentAbilityList = ListAbilityOfficialCHS
                CurrentAbility = AbilityNamesOfficialCHS
            Case ChineseDisplayConfig.OfficialChineseTraditional
                CurrentAbilityList = ListAbilityOfficialCHT
                CurrentAbility = AbilityNamesOfficialCHT
        End Select
    End Sub
